// Place your key bindings in this file to override the defaults
[
    {
        "key": "escape",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "!editorTextFocus"
    },
    {
        "key": "ctrl+E",
        "command": "workbench.action.showAllEditorsByMostRecentlyUsed",
    },
    {
        "key": "ctrl+1",
        "command": "workbench.files.action.focusFilesExplorer",
    },
    {
        "key": "ctrl+1",
        "command": "workbench.action.closeSidebar",
        "when": "explorerViewletVisible"
    },
    {
        "key": "ctrl+2",
        "command": "workbench.action.toggleAuxiliaryBar",
    },
    {
        "key": "ctrl+3",
        "command": "workbench.action.togglePanel",
    },
    {
        "key": "ctrl+4",
        "command": "",
    },
    {
        "key": "ctrl+5",
        "command": "workbench.action.toggleZenMode",
    },
    {
        "key": "ctrl+b",
        "command": "runCommands",
        "args": {
            "commands": [
                "workbench.action.closePanel",
                "workbench.action.closeSidebar",
                "workbench.action.closeAuxiliaryBar",
            ]
        }
    },
    {
        "key": "ctrl+alt+backspace",
        "command": "runCommands",
        "args": {
            "commands": [
                "editor.action.deleteLines",
                "cursorUp",
                "cursorLineEnd"
            ],
        },
        "when": "textInputFocus && !editorReadonly"
    },
    {
        "key": "home",
        "command": "workbench.action.navigateBack",
        "when": "editorTextFocus"
    },
    {
        "key": "end",
        "command": "workbench.action.navigateForward",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+t",
        "command": "editor.action.refactor",
        "when": "editorHasCodeActionsProvider && textInputFocus && !editorReadonly"
    },
    {
        "key": "alt+b",
        "command": "csdevkit.rebuildSolution"
    },
    {
        "key": "ctrl+oem_5",
        "command": "solutionExplorer.focus",
    },
    {
        "key": "ctrl+oem_5",
        "command": "workbench.action.maximizeEditorHideSidebar",
        "when": "solutionExplorer"
    },
    {
        "key": "ctrl+oem_2",
        "command": "editor.action.commentLine",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+[Quote]",
        "command": "editor.action.removeBrackets",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+up",
        "command": "editor.action.smartSelect.expand",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+down",
        "command": "editor.action.smartSelect.shrink",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+ctrl+k",
        "command": "-editor.action.deleteLines",
        "when": "textInputFocus && !editorReadonly"
    },
    {
        "key": "ctrl+k",
        "command": "extension.conventionalCommits"
    },
    {
        "key": "shift+ctrl+g",
        "command": "git-graph.view"
    },
    {
        "key": "ctrl+d",
        "command": "editor.action.addSelectionToNextFindMatch",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+h",
        "command": "workbench.view.scm",
        "when": "workbench.scm.active"
    },
    {
        "key": "alt+shift+g",
        "command": "-workbench.view.scm",
        "when": "workbench.scm.active"
    },
    {
        "key": "shift+ctrl+w",
        "command": "workbench.action.openWorkspaceSettingsFile"
    },
    {
        "key": "alt+shift+f5",
        "command": "csdevkit.debug.noDebugFileLaunch"
    },
    {
        "key": "alt+f5",
        "command": "csdevkit.debug.fileLaunch"
    },
    {
        "key": "ctrl+backspace",
        "command": "csdevkit.deleteItem",
        "when": "focusedView == 'solutionExplorer' && selectedSolutionExplorerNodeType == 'File' || focusedView == 'solutionExplorer' && selectedSolutionExplorerNodeType == 'Folder'"
    },
    {
        "key": "delete",
        "command": "-csdevkit.deleteItem",
        "when": "focusedView == 'solutionExplorer' && selectedSolutionExplorerNodeType == 'File' || focusedView == 'solutionExplorer' && selectedSolutionExplorerNodeType == 'Folder'"
    },
    {
        "key": "ctrl+backspace",
        "command": "csdevkit.removeItem",
        "when": "focusedView == 'solutionExplorer' && selectedSolutionExplorerNodeType == 'LinkedFile'"
    },
    {
        "key": "delete",
        "command": "-csdevkit.removeItem",
        "when": "focusedView == 'solutionExplorer' && selectedSolutionExplorerNodeType == 'LinkedFile'"
    },
    {
        "key": "ctrl+backspace",
        "command": "csdevkit.removeProject",
        "when": "focusedView == 'solutionExplorer' && selectedSolutionExplorerNodeType == 'Project'"
    },
    {
        "key": "delete",
        "command": "-csdevkit.removeProject",
        "when": "focusedView == 'solutionExplorer' && selectedSolutionExplorerNodeType == 'Project'"
    },
    {
        "key": "ctrl+backspace",
        "command": "csdevkit.removeSolutionFolder",
        "when": "focusedView == 'solutionExplorer' && selectedSolutionExplorerNodeType == 'SolutionFolder'"
    },
    {
        "key": "delete",
        "command": "-csdevkit.removeSolutionFolder",
        "when": "focusedView == 'solutionExplorer' && selectedSolutionExplorerNodeType == 'SolutionFolder'"
    },
    {
        "key": "ctrl+g",
        "command": "-editor.action.nextMatchFindAction",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+g",
        "command": "workbench.action.gotoLine"
    },
    {
        "key": "alt+g",
        "command": "-workbench.action.gotoLine"
    },
    {
        "key": "shift+ctrl+t",
        "command": "workbench.view.testing.focus"
    },
    {
        "key": "ctrl+r ctrl+t",
        "command": "testing.runAtCursor",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+ctrl+[Comma] c",
        "command": "-testing.runAtCursor",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+d ctrl+t",
        "command": "testing.debugAtCursor",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+ctrl+[Comma] ctrl+c",
        "command": "-testing.debugAtCursor",
        "when": "editorTextFocus"
    }
]